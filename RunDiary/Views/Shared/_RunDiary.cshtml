<!DOCTYPE html>

<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>
    @Styles.Render("~/Content/css")
    @Styles.Render("~/Content/themes/base/datepicker.css")
   @Styles.Render("~/fullcalendar-2.5.0/fullcalendar.css")


</head>
<body>
    @Html.Partial("_Nav")
    <div>
        @RenderBody()
    </div>
    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @Scripts.Render("~/bundles/modernizr")
    @Scripts.Render("~/Scripts/jquery-ui-1.11.4.js")
    @Scripts.Render("~/Scripts/main.js")
    @Scripts.Render("~/fullcalendar-2.5.0/lib/moment.min.js")
    @Scripts.Render("~/fullcalendar-2.5.0/fullcalendar.js")
</body>
</html>
